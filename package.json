  },
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "ignore": [
      "/**/*.js"
    ]
  },
  "scripts": {
    "book": "gitbook install && gitbook build",
    "book:serve": "gitbook install && gitbook serve",
    "book:static": "rm -rf app/book && npm run book && cp -r _book app/book",
    "build": "webpack --config webpack.config.prod.js",
    "db:clean": "node scripts/db-clean.js",
    "serve": "npm run build && node server.js",
    "dev": "node server.js",
    "tdd": "mocha --opts mocha.opts --watch --watch-extensions js",
    "test": "mocha --opts mocha.opts"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel": "^6.3.26",
    "babel-loader": "^6.2.2",
