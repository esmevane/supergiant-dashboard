    "morgan": "^1.7.0",
    "node-neat": "^1.7.2",
    "node-sass": "^3.4.2",
    "normalizr": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "proxy-middleware": "^0.15.0",
